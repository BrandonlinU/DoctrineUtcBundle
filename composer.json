{
  "name": "brandonlinu/doctrine-utc-bundle",
  "type": "symfony-bundle",
  "description": "A Symfony Bundler to manage date and time in UTC with timezone support for Doctrine",
  "keywords": ["Symfony", "UTC", "DateTime", "Doctrine"],
  "homepage": "https://github.com/BrandonlinU/DoctrineUtcBundle",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Brandon Antonio",
      "email": "brandolin_a_@outlook.com",
      "homepage": "https://github.com/BrandonlinU"
    }
  ],
  "scripts": {
    "test": "@php vendor/bin/simple-phpunit"
  },
  "require": {
    "php": ">=8.1",
    "symfony/dependency-injection": "^6.4 || ^7.0"
  },
  "require-dev": {
    "doctrine/doctrine-bundle": "^2.0",
    "doctrine/doctrine-fixtures-bundle": "^4.1",
    "doctrine/orm": "^2.0 || ^3.3",
    "liip/test-fixtures-bundle": "^3.3",
    "symfony/framework-bundle": "^6.4 || ^7.0",
    "symfony/http-kernel": "^6.4 || ^7.0",
    "symfony/phpunit-bridge": "^6.4.16 || ^7.0"
  },
  "autoload": {
    "psr-4": {
      "BrandonlinU\\DoctrineUtcBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BrandonlinU\\DoctrineUtcBundle\\Test\\": "test/"
    }
  },
  "suggest": {
    "doctrine/orm": "To use the embeddables UtcDateTime and UtcDateTimeImmutable"
  },
  "conflict": {
    "doctrine/doctrine-bundle": "<2.0 || >=3.0"
  },
  "config": {
    "sort-packages": true
  }
}
